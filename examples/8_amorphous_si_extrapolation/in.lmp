units        metal
processors      * * *
boundary p p p
atom_style    atomic
atom_modify map yes
box tilt large

read_data structures/1728_atom/aSiGe_C${conc}.lmp

pair_style    tersoff
pair_coeff    * * forcefields/SiCGe.tersoff Si(D) Ge
mass            1 28.06 # Si
mass            2 72.64 # Ge

fix com all momentum 1 linear 1 1 1

print 'calculating dynamical_matrix'
dynamical_matrix all eskm 0.00001 file structures/1728_atom/Dyn_C${conc}.form binary no

print 'calculating third_order'
third_order all eskm 0.00001 file structures/1728_atom/THIRD_C${conc} binary no
